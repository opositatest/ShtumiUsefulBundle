<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="shtumi_ajaxautocomplete" path="/shtumi_ajaxautocomplete">
        <default key="_controller">ShtumiUsefulBundle:AjaxAutocompleteJSON:getJSON</default>
    </route>

    <route id="shtumi_select2_entity" path="/shtumi_select2_entity">
        <default key="_controller">ShtumiUsefulBundle:Select2Entity:getJSON</default>
    </route>

    <route id="shtumi_ajaxfileupload" path="/shtumi_ajaxfileupload">
        <default key="_controller">ShtumiUsefulBundle:AjaxFile:upload</default>
    </route>

    <route id="shtumi_dependent_filtered_entity" path="/shtumi_dependent_filtered_entity">
        <default key="_controller">%shtumi_dependent_filtered_entity_controller_route%</default>
    </route>

    <route id="shtumi_dependent_filtered_select2" path="/shtumi_dependent_filtered_select2">
        <default key="_controller">ShtumiUsefulBundle:DependentFilteredEntity:getJson</default>
    </route>

</routes>